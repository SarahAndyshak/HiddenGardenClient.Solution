@{
  ViewData["Title"] = $"Details for {Model.Service}";
}

<h1>All About @Model.Service</h1>

<p>Description: @Model.Description</p>
<p>Location: @Model.Address</p>
<p>Instructions: @Model.Instructions</p>

<p>@Html.ActionLink("Edit details", "Edit", new { id = @Model.BackyardId })</p>
<p>@Html.ActionLink("Delete backyard", "Delete", new { id = @Model.BackyardId })</p>
<p>@Html.ActionLink("Return to all backyards", "Index")</p>